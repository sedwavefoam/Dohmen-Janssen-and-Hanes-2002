FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-10;

boundaryField
{
    inlet
    {
    type            inletOutlet;
	inletValue	uniform 1e-10;
	value		uniform 1e-10;	
    }
    bottom
    {
        type            zeroGradient;
    }
    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 1e-10;
        value           uniform 1e-10;
    }
    frontandback
    {
        type            empty;
    }
}


// ************************************************************************* //
