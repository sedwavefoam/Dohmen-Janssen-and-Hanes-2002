FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nutb;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 9e-12;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 9e-12;
    }
    bottom
    {
        type            calculated;
        value           uniform 9e-12;
    }
    atmosphere
    {
        type            calculated;
        value           uniform 9e-12;
        
    }
    frontandback
    {
        type            empty;
    }
}


